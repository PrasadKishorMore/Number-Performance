 int num = 121;
      
      int rev = 0;
      
      while(num > 0){
        
        int digit = num % 10;
        
        rev = rev * 10 + digit;
        
        num /= 10;
        
      }
      
      // if(num == rev ){
        
      //   System.out.println(true);
      // }else{
      //   System.out.println(false);
      // }
      
      System.out.println(rev);
